.row#start-project-first-row
	.container
		.row
			.span2
				= link_to image_tag("mobb_logo.png", class: "brand", style: "margin-top: 10px;"), root_path
			.span7
				%ul.nav.nav-tabs
					%li#about
						= link_to "About Idea Mobb", static_pages_about_path, id: "about"
					%li#browse
						= link_to "Browse", static_pages_browse_path, id: "browse"
					%li.active#start					
						= link_to "Start", static_pages_start_project_path, id: "start"
			.span3
				.row-fluid
					.span12
						%form
							%button.btn.btn-small.btn-success Log in
							%button.btn.btn-small.btn-primary Sign up
				.row-fluid
					.span12
						%form.form-search
							%input.input-medium.search-query{type: "text",placeholder: "To search type..."}
							%i.icon-search

